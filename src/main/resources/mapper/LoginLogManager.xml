<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tree.clouds.coordination.mapper.LoginLogMapper">


    <select id="loginLogPage" resultType="com.tree.clouds.coordination.model.entity.LoginLog">
        select * from login_log
        where 1=1
        <if test="loginLogPageVO.account!=null and loginLogPageVO.account!=''">
            and account like "%"#{loginLogPageVO.account}"%"
        </if>
        <if test="loginLogPageVO.ip!=null and loginLogPageVO.ip!=''">
            and ip like "%"#{loginLogPageVO.ip}"%"
        </if>
        <if test="loginLogPageVO.loginStartTime!=null and loginLogPageVO.loginStartTime!=''">
            and creatd_time >= #{loginLogPageVO.loginStartTime}
        </if>
        <if test="loginLogPageVO.loginEndTime!=null and loginLogPageVO.loginEndTime!=''">
            and creatd_time &lt;= #{loginLogPageVO.loginEndTime}
        </if>
    </select>
    <select id="loginInfo" resultType="com.tree.clouds.coordination.model.entity.LoginLog">
        select * from login_log where account=#{userAccount} order by CREATED_TIME DESC limit 1
    </select>
</mapper>
